{
  "name": "leshi-ui",
  "version": "0.0.10",
  "description": "A CLI for adding React Native UI components to your project",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "leshi-ui": "dist/index.js"
  },
  "files": [
    "dist",
    "packages"
  ],
  "scripts": {
    "build": "tsc && npm run copy-packages",
    "copy-packages": "cp -r ../packages ./packages",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "chalk": "^5.3.0",
    "zod": "^3.22.4",
    "@babel/core": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "@babel/types": "^7.23.0",
    "prompts": "^2.4.2",
    "ora": "^7.0.1",
    "execa": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/babel__core": "^7.20.0",
    "@types/babel__parser": "^7.1.1",
    "@types/babel__traverse": "^7.20.0",
    "@types/babel__generator": "^7.6.0",
    "@types/prompts": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "jest": "^29.6.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.0",
    "memfs": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "react-native",
    "ui",
    "components",
    "cli",
    "shadcn",
    "expo",
    "unistyles"
  ],
  "author": "Agustin Oberg",
  "license": "MIT"
}
